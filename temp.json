{
	"$schema": "http://json-schema.org/schema#",
	"type": "object",
	"properties": {
		"data": {
			"type": "object",
			"properties": {
				"youtube": {
					"type": "object",
					"properties": {
						"search": {
							"type": "array",
							"items": {
								"anyOf": [{
									"type": "null"
								}, {
									"type": "object",
									"properties": {
										"info": {
											"type": "object",
											"properties": {
												"videoInfo": {
													"type": "array",
													"items": {
														"type": "object",
														"properties": {
															"statistics": {
																"type": "object",
																"properties": {
																	"viewCount": {
																		"type": "integer"
																	}
																},
																"required": ["viewCount"]
															}
														},
														"required": ["statistics"]
													}
												}
											},
											"required": ["videoInfo"]
										}
									},
									"required": ["info"]
								}]
							}
						}
					},
					"required": ["search"]
				}
			},
			"required": ["youtube"]
		}
	},
	"required": ["data"]
}